prototype(TYPO3.Neos:Page) {

    head {

        gmapsApiJs = TYPO3.TypoScript:Tag {
            @position = 'before javascripts'
            tagName = 'script'
            attributes {
                type = 'text/javascript'
                src = ${'https://maps.googleapis.com/maps/api/js?key='+Configuration.settings('ObisConcept.NeosGmaps.apiKey')}
            }
        }

        gmapsJs = TYPO3.TypoScript:Tag {
            @position = 'after javascripts'
            tagName = 'script'
            attributes {
                id = 'bootstrap-js'
                type = 'text/javascript'
                src = TYPO3.TypoScript:ResourceUri {
                    path = 'resource://ObisConcept.NeosGmaps/Public/JavaScript/main.min.js'
                }
            }
        }

        gmapsBackendJs = TYPO3.TypoScript:Tag {
            @position = 'after javascripts'
            tagName = 'script'
            attributes {
                type = 'text/javascript'
                src = TYPO3.TypoScript:ResourceUri {
                    path = 'resource://ObisConcept.NeosGmaps/Public/JavaScript/backend.min.js'
                }
            }
            @if.onlyRenderWhenNotInLiveWorkspace = ${node.context.workspace.name != 'live'}
        }

    }

}